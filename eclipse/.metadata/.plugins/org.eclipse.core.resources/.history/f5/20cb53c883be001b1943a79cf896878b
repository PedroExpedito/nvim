package oo.heranca.desafio;

public class Ferrari extends Carro implements Esportivo, Luxo {
	
	public Ferrari() {
		super(300);
	}
	@Override // anotation
	public void acelerar() {
		this.aceleracao = 15;
		super.acelerar();
	}

	@Override
	public void ligarTurbo() {
		aceleracao = 35;
		
	}
	@Override
	public void desligarTurbo() {
		aceleracao = 5;
	}

}
