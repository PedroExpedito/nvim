package desafio.heranca;

public class Carro {

	double velocidade = 0;
	double aceleracao = 5;
	double freio = 5;
	final int VELOCIDADE_MAXIMA;

	void acelerar() {
		if (velocidade < 100) {
			velocidade += aceleracao;
		}
	}

	void freiar() {
		if (velocidade > 0) {
			velocidade -= freio;
		}
	}

}
