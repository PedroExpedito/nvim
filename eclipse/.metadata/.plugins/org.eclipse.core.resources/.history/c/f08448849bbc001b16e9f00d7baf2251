package desafios.oo.composicao;

public class Main {
	public static void main(String[] args) {
		Cliente cliente1 = new Cliente("Pedro");
		
		
		Produto produto1 = new Produto("Laranja", 2.50);
		Item item1 = new Item(2, produto1);
		
		
		item1.quantidade = 2;
		item1.produto = produto1;

		Compra compra1 = new Compra();

		compra1.lista.add(item1);
		
		cliente1.lista.add(compra1);
		
		System.out.println(cliente1.valorTotal());
		
		
	}

}
